useEffect(() => {
    const storedUser = JSON.parse(sessionStorage.getItem('user'));
    const fetchDonorData = async () => {
        donorService.viewDonorById(storedUser.id)
        .then((resp) => {
            console.log(resp);
            setUser( user => ({
                ...user,
                name : resp.data.name,
                email : resp.data.email,
            }))
            
        })
        .catch((err) =>{
            console.log(err);
        })
    };

    fetchDonorData();
  }, []);
